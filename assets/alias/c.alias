#
#  files used by ../lib/alias
#  aliases for all systemctl and journalctl
#

     :  systemctl
-    :  systemctl *
dis  :  systemctl disable *
dr   :  systemctl daemon-reload
en   :  systemctl enable *
j    :  journalctl -u * -b
jf   :  journalctl -u * -b -f
m    :  systemctl mask *
rl   :  systemctl reload *
rs   :  systemctl restart *
sa   :  systemctl start *
so   :  systemctl stop *
st   :  systemctl status *
u    :  systemctl unmask *
